{
  "name": "tidalcycles",
  "main": "./lib/tidalcycles",
  "version": "0.8.4",
  "description": "Atom package for version 0.7 and later of TidalCycles, a language for live coding pattern. This is a new version of the 'tidal' plugin, which you will need to uninstall before using this one",
  "activationCommands": {
    "atom-text-editor": [
      "tidalcycles:eval",
      "tidalcycles:eval-multi-line",
      "tidalcycles:eval-multi-line-multi-block",
      "tidalcycles:eval-all-lines"
    ],
    "atom-workspace": "tidalcycles:boot"
  },
  "dependencies": {
    "atom-space-pen-views": "^2.0.3"
  },
  "repository": "https://github.com/tidalcycles/atom-tidalcycles",
  "license": "GPL-3.0",
  "engines": {
    "atom": ">=0.174.0 <2.0.0"
  }
}
